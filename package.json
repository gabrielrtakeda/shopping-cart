{
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "yarn update-schema && yarn build && yarn server:start",
    "server:start": "babel-node ./server/index.js",
    "build": "relay-compiler --src ./client/ --schema ./server/data/schema.graphql",
    "update-schema": "babel-node ./server/utils/updateSchema.js",
    "prettier": "find . \\( -name node_modules -o -name __generated__ \\) -prune -or \\( -name '*.js' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write",
    "lint": "./node_modules/.bin/eslint --fix ./client ./server"
  },
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-relay": "^1.7.0",
    "classnames": "2.2.6",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.8.3",
    "nodemon": "^1.18.6",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-relay": "^1.7.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "relay-compiler": "^1.7.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
