    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.4.3",
    "nodemon": "^1.18.6",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
